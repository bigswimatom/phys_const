(defun const ()
  "insert physical constant"
  (interactive)
  (let ((key (completing-read "const?: " const-alist nil t)) list)
    (setq list (assoc key const-alist))
    (if list
        (insert 
         (format "%s" (car (cdr list))))
      )))

